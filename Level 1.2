#include<iostream>
#include<cmath)
using namespace std;
int toplama(int a,int b){
    return a+b;
}

int çıxma(int a,int b){
    return a-b;
}

int vurma(int a,int b){
    return a*b;
}

int bölmə(int a,int b){
    return a/b;
}

int main() {
   cout<<"bir eded daxil edin:";
   cin>>a>>b;

   cout<<"toplama"<<toplama(a,b)<<endl;
   cout<<"çıxma"<<çıxma(a,b)<<endl;
   cout<<"vurma"<<vurma(a,b)<<endl;
   cout<<"bölmə"<<bölmə(a,b)<<endl;


    return 0;
}
